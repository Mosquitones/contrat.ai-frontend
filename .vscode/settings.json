{
  "prettier.configPath": ".prettierrc",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "javascript.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    "extensions": [".js", ".ts", ".jsx", ".tsx"]
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.language": "en,pt,pt-BR",
  "cSpell.enableFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.words": [
    "airbnb",
    "backoffice",
    "camelcase",
    "corejs",
    "deepmerge",
    "docgen",
    "Docgen",
    "Eduplayition",
    "eduplaytion",
    "formik",
    "fusionauth",
    "FUSIONAUTH",
    "ijourney",
    "languagedetector",
    "matematik",
    "notistack",
    "Parens",
    "rehype",
    "scroller",
    "svgr",
    "tanstack",
    "testid"
  ]
}
